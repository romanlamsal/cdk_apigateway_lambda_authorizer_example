{
  "dependencies": {
    "@aws-cdk/aws-kms": "^1.56.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "24.0.23",
    "@types/node": "^13.13.1",
    "@types/request": "^2.48.4",
    "babel-jest": "^25.4.0",
    "dotenv": "^8.2.0",
    "jest": "^25.4.0",
    "node": "^13.13.0",
    "testcontainers": "^2.8.0",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "license": "MIT",
  "main": "index.js",
  "name": "backend",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc && cp package.json build/package.json && cp yarn.lock build/yarn.lock && cd build && yarn install --production && rm package.json yarn.lock && rm -rf test",
    "deploy": "cd ../terraform && terraform apply",
    "test": "jest --watch",
    "test:coverage": "jest --watchAll=false --coverage",
    "test:integration:win": "yarn jest --config jest/integrationtest.config.js --watchAll=false",
    "checkAll": "tsc --noEmit && yarn test:coverage"
  },
  "version": "1.0.0",
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/integrationTest"
    ],
    "collectCoverageFrom": [
      "src/impl/**/*",
      "src/types/**/*"
    ]
  }
}
