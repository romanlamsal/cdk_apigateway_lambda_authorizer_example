{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    // override tslint-react rules here
    "jsx-wrap-multiline": false,
    "jsx-alignment": false,
    "semicolon": false,
    "quotemark": false,
    "typedef": true,
    "interface-name": false,
    "no-trailing-whitespace": false,
    "jsx-no-bind": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "trailing-comma": false
  }
}